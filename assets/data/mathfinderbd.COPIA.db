BEGIN TRANSACTION;
CREATE TABLE IF NOT EXISTS "PERSONAJE" (
	"IDPERSO"	INTEGER,
	"NOMBRE"	TEXT,
	"ATQ1"	INTEGER DEFAULT 0,
	"ATQ2"	INTEGER DEFAULT 0,
	"ATQ3"	INTEGER DEFAULT 0,
	"ATQ4"	INTEGER DEFAULT 0,
	"ATQ5"	INTEGER DEFAULT 0,
	"FUE"	INTEGER DEFAULT 10,
	"DES"	INTEGER DEFAULT 10,
	"CA"	INTEGER DEFAULT 10,
	"NIVEL"	INTEGER,
	"TSREF"	INTEGER,
	"TSFOR"	INTEGER,
	"TSVOL"	INTEGER,
	PRIMARY KEY("IDPERSO" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "BONUS" (
	"IDBONUS"	INTEGER,
	"NOMBRE"	TEXT NOT NULL,
	"TIPO1"	TEXT,
	"TIPO2"	TEXT,
	"MOD1"	INTEGER DEFAULT 0,
	"MOD2"	INTEGER DEFAULT 0,
	PRIMARY KEY("IDBONUS" AUTOINCREMENT)
);
CREATE TABLE IF NOT EXISTS "PERSOBONUS" (
	"IDPERSO"	INTEGER,
	"IDBONUS"	INTEGER,
	PRIMARY KEY("IDPERSO","IDBONUS")
);
